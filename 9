import math as m
class Solution:
    def isPalindrome(self, x: int) -> bool:
        if x<0:
          return None
        num = str(x)
        print(num)
        l=len(num)
        for i in range (m.floor(l/2)):
          print(num[i])
          if num[i]!=num[l-1-i]:
            return None
        return True
          
