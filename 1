class Solution:
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        """
        #### Brute Force
        for i in range(len(nums)):
          for j in range(i+1,len(nums)):
            if nums[i]+nums[j]==target :
              return [i,j]
            
          
        return -1
        """
        dictArray={}
        for i in range(len(nums)):
          if nums[i] in dictArray:
            dictArray(nums[i][1])={i}
          else:
            dictArray[nums[i][0]]={i}
          
        print(dictArray)
        
        for i in nums:
          print(i)
          if target-i>0 and i in dictArray and target-i in dictArray:
            return [dictArray[i], dictArray[target-i]]
            
        
